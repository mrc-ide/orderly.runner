{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "branches": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "commit_hash": {
                        "type": "string"
                    },
                    "message": {
                        "type": "string"
                    },
                    "time": {
                        "type": "number"
                    }
                },
                "required": [
                    "name",
                    "commit_hash",
                    "message",
                    "time"
                ]
            }
        },
        "default_branch": {
            "type": "string"
        }
    },
    "required": [
        "branches",
        "default_branch"
    ]
}
